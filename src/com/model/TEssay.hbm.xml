<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.TEssay" table="t_essay">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="userId" type="java.lang.Integer">
            <column name="userId" />
        </property>
        <property name="acId" type="java.lang.Integer">
            <column name="acId" />
        </property>
        <property name="biaoti" type="java.lang.String">
            <column name="biaoti" length="100" />
        </property>
        <property name="neirong" type="java.lang.String">
            <column name="neirong" length="5000" />
        </property>
        <property name="fabusj" type="java.lang.String">
            <column name="fabusj" length="20" />
        </property>
    </class>
</hibernate-mapping>
